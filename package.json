  "repository": {
    "type": "git",
    "url": "git+https://github.com/danlutz/MCI_eCollaboration_WS2019.git"
  },
  "scripts": {
    "clean": "rimraf .cache public",
    "build": "npm run clean && npm run lambda-build && gatsby build",
    "lambda-serve": "netlify-lambda serve lambda/src -c lambda/webpack.lambda.js",
    "lambda-build": "netlify-lambda build lambda/src",
    "develop": "npm run clean && concurrently --kill-others \"npm run lambda-serve\" \"gatsby develop\"",
    "start": "npm run develop",
    "lint": "eslint --ext .js,.ts,.tsx src lambda/src",
    "prettier": "prettier \"**/*.+(js|jsx|json|yml|yaml|html|css|less|scss|ts|tsx|md|mdx|graphql|vue|py|java|php)\"",
    "format": "npm run prettier -- --write",
    "validate": "npm run lint && npm run prettier -- --list-different",
    "jest": "jest",
    "test": "jest --watch"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "gatsby": "^2.15.28",
    "gatsby-plugin-layout": "^1.1.10",
    "gatsby-plugin-manifest": "^2.2.20",
    "gatsby-plugin-netlify-cms": "^4.1.21",
    "gatsby-plugin-react-helmet": "^3.1.10",
    "gatsby-plugin-react-svg": "^2.1.2",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sass": "^2.1.17",
    "gatsby-plugin-typescript": "^2.1.11",
    "gatsby-source-filesystem": "^2.1.28",
    "gatsby-transformer-remark": "^2.6.26",
    "intl": "^1.2.5",
    "netlify-cms-app": "^2.9.7",
    "node-sass": "^7.0.0",
    "react": "^16.10.0",
    "react-dom": "^16.10.0",
    "react-helmet": "^5.2.1",
    "reactstrap": "^8.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-typescript": "^7.6.0",
    "@types/aws-lambda": "^8.10.33",
